import java.util.Scanner;
class s{
  pubulic static void main(String s[]){
    Scanner input=new Scanner(System.in);
    int rows=input.nextInt();
    int cols=input.nextInt();
    for(int i=0;i<rows;i++){
      for(int j=0;j<cols;j++){
        System.out.print("* ");
        }
      System.out.println();
      }
    }
  }
