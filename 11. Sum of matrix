import java.util.Scanner;
class A{
public static void main(String s[]){
Scanner input=new Scanner(System.in);
int mat1={[1,2],[3,4]};
int mat2={[3,4],[5,6]};
int sum[][]=int mat[2][2];
int len=mat1.length;
for(int i=0;i<len;i++){
for(int j=0;j<len;j++){
sum[i][j]=mat1[i][j]+mat2[i][j];
System.out.print(sum[i][j]+"\t");
}
system.out.println();
}
}
}
